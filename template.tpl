  code: |-
    const mockInjectScript = createMockInjectScript();

    // Call runCode to run the template's code.
    runCode(mockData);

    // Verify that the tag finished successfully.
    assertThat(mockInjectScript.calls[0][0]).isEqualTo(RCMX_TAG_URL);
    assertThat(mockInjectScript.calls[0][3]).isEqualTo(RCMX_KEY);
    assertThat(mockInjectScript.calls.length).isEqualTo(1);
setup: |-
  const mockData = {
    stage_id: 0,
    lang: 'en-GB',
    size: 'm',
  };

  const RCMX_KEY = 'rcm1x';
  const RCMX_TAG_URL = 'https://getrcmx.com/i/tag.js';
  const RCMX_LOADED_KEY = 'rcm1x_init';

  // Helpers
  function createMockInjectScript () {
    const calls = [];
    function fn() { calls.push(arguments); }
    mock('injectScript', fn);
    return { calls: calls };
  }


